#include<stdio.h>
#include<stdlib.h>

int main(void){
	
	int a,c,d,e;
	float b;
	
	printf("Enter item number: ");
	scanf("%d",&a);
	printf("Enter unit price: ");
	scanf("%f",&b);
	printf("Enter purchase date (mm/dd/yyyy): ");
	scanf("%d/%d/%d",&c,&d,&e);
	
	printf("Item\tUnit\tPurchase\n\tPrice\tDate\n");
	printf("%d\t$%.2f\t%d/%d/%d\n",a,b,c,d,e);

	return 0;	
}
